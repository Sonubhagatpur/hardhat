    function updateOrder(
        address _nftAddress,
        uint256 _tokenId,
        uint256 _value,
        uint256 _priceInWei,
        uint256 _expiresAt
    ) external whenNotPaused {
        Order storage order = orderBytokenId[_nftAddress][_tokenId];

        // Check valid order to update
        require(order.id != 0, "Marketplace: asset not published");
        require(order.seller == msg.sender, "Marketplace: sender not allowed");
        require(
            order.expiresAt >= block.timestamp,
            "Marketplace: order expired"
        );

        // check order updated params
        require(_priceInWei > 0, "Marketplace: Price should be bigger than 0");
        require(
            _expiresAt > block.timestamp.add(1 minutes),
            "Marketplace: Expire time should be more than 1 minute in the future"
        );

        order.price = _priceInWei;
        order.expiresAt = _expiresAt;
        order.value = order.value + _value;

        emit OrderUpdated(order.id, _tokenId, _value, _priceInWei, _expiresAt);
    }

    // Function to cancle listed NFT
    function cancelOrder(
        address _nftAddress,
        uint256 _tokenId,
        uint256 _value
    ) external whenNotPaused {
        Order memory order = orderBytokenId[_nftAddress][_tokenId];

        require(
            order.seller == msg.sender || msg.sender == owner(),
            "Marketplace: unauthorized sender"
        );

        // Remove pending bid if any
        Bid memory bid = bidByOrderId[_nftAddress][_tokenId];

        if (bid.id != 0) {
            _cancelBid(
                bid.id,
                _nftAddress,
                _tokenId,
                _value,
                bid.bidder,
                bid.price
            );
        }

        //  Cancel order.
        _cancelOrder(order.id, _nftAddress, _tokenId, _value, msg.sender);
    }

    //Function to place a Bid of  listed NFT
    function safePlaceBid(
        address _nftAddress,
        uint256 _tokenId,
        uint256 _value,
        uint256 _priceInWei,
        uint256 _expiresAt
    ) external payable nonReentrant whenNotPaused {
        Order memory order = _getValidOrder(_nftAddress, _tokenId);

        order.currency == MARKETPLACE_ETHER
            ? _createBid(_nftAddress, _tokenId, _value, msg.value, _expiresAt)
            : _createBid(
                _nftAddress,
                _tokenId,
                _value,
                _priceInWei,
                _expiresAt
            );
    }

    function acceptBid(
        address _nftAddress,
        uint256 _tokenId,
        uint256 _value,
        uint256 _priceInWei
    ) public whenNotPaused {
        // check order validity
        Order memory order = _getValidOrder(_nftAddress, _tokenId);

        require(
            orderBytokenId[_nftAddress][_tokenId].value >= _value,
            "AcceptBid: Order is not listed"
        );

        // item seller is the only allowed to accept a bid
        require(order.seller == msg.sender, "Marketplace: unauthorized sender");

        Bid memory bid = bidByOrderId[_nftAddress][_tokenId];

        require(
            bidByOrderId[_nftAddress][_tokenId].value >= _value,
            "AcceptBid: Bid value doesn't exist!"
        );

        require(bid.price == _priceInWei, "Marketplace: invalid bid price");
        require(
            bid.expiresAt >= block.timestamp,
            "Marketplace: the bid expired"
        );

        // remove bid

        if (_value < bidByOrderId[_nftAddress][_tokenId].value) {
            bidByOrderId[_nftAddress][_tokenId].value -= _value;
        } else {
            delete bidByOrderId[_nftAddress][_tokenId];
        }

        emit BidAccepted(bid.id);

        // Send market fees to owner

        if (commission > 0) {
            // Calculate sale share
            require(
                commission < bid.price,
                "Marketplace:  commission exceeds order price"
            );

            // Transfer share value for marketplace Owner
            order.currency == MARKETPLACE_ETHER
                ? payable(owner()).transfer(commission)
                : IERC20(order.currency).safeTransfer(owner(), commission);
        }

        // royalty fee to cut
        uint256 royaltyvalue = 0;

        if (order.royalty != 0) {
            // Calculate royalty value
            royaltyvalue = bid.price.mul(order.royalty).div(10000);

            if (VintageNFT(order.nftAddress).minter(_tokenId) != bid.bidder) {
                // Transfer royalty percentage to nft minter

                order.currency == MARKETPLACE_ETHER
                    ? payable(
                        address(VintageNFT(order.nftAddress).minter(_tokenId))
                    ).transfer(royaltyvalue)
                    : IERC20(order.currency).safeTransfer(
                        address(VintageNFT(order.nftAddress).minter(_tokenId)),
                        royaltyvalue
                    );
            }
        }
        // transfer  bid value minus market fee and royality to seller

        order.currency == MARKETPLACE_ETHER
            ? payable(order.seller).transfer(
                bid.price.sub(commission).sub(royaltyvalue)
            )
            : IERC20(order.currency).safeTransfer(
                order.seller,
                bid.price.sub(commission).sub(royaltyvalue)
            );

        _executeOrder(
            order.id,
            bid.bidder,
            _nftAddress,
            _tokenId,
            _value,
            _priceInWei
        );
    }

    function _createOrder(
        address _nftAddress,
        uint256 _tokenId,
        uint256 _value,
        uint256 _priceInWei,
        uint256 _royalty,
        uint256 _expiresAt,
        address _currency
    ) public {
        require(isCreateCourse[msg.sender],"not valid address");
        // Check ERC-1155 NFT registry
        VintageNFT nftRegistry = VintageNFT(_nftAddress);

        // Check _acceptedCurrency
        require(
            acceptedCurrencies[_currency],
            "Marketplace: Unacceptable marketplace currency"
        );

        // Check if the caller is the owner of the specified token fractions
        require(
            nftRegistry.balanceOf(msg.sender, _tokenId) >= _value,
            "Marketplace: Insufficient token balance"
        );

        // require(_priceInWei > 0, "Marketplace: Price should be bigger than 0");
    
        require(
            _expiresAt > block.timestamp.add(1 minutes),
            "Marketplace: Publication should be more than 1 minute in the future"
        );

        // Check if the nft discount is set and active
        if (isNftDiscountActive) {
            uint256 storedNftDiscount = nftDiscountMapping[_tokenId];

            if (storedNftDiscount > 0) {
                _priceInWei = _priceInWei.sub(
                    (_priceInWei.mul(storedNftDiscount)).div(10000)
                );
            }
        }

        // Transfer the specified fractions from the owner to the marketplace contract
        nftRegistry.safeTransferFrom(
            msg.sender,
            address(this),
            _tokenId,
            _value,
            ""
        );

        // Create the orderId
        bytes32 orderId = keccak256(
            abi.encodePacked(
                block.timestamp,
                msg.sender,
                _nftAddress,
                _tokenId,
                _priceInWei
            )
        );

        // Save royalty information for the tokenId
        royaltyBytokenId[_nftAddress][_tokenId] = _royalty;

        // Save the order
        orderBytokenId[_nftAddress][_tokenId] = Order({
            id: orderId,
            seller: msg.sender,
            nftAddress: _nftAddress,
            tokenId: _tokenId,
            value: _value,
            price: _priceInWei,
            royalty: _royalty,
            expiresAt: _expiresAt,
            currency: _currency
        });

        emit OrderCreated(
            orderId,
            msg.sender,
            _nftAddress,
            _tokenId,
            _value,
            _priceInWei,
            _expiresAt,
            _currency
        );
    }

    function createMultipleOrder(
        address _nftAddress,
        uint256[] memory _tokenIds,
        uint256[] memory _values,
        uint256 _priceInWei,
        uint256 _royalty,
        uint256 _expiresAt,
        address _currency
    ) public onlyOwner{
        // Check ERC-1155 NFT registry
        VintageNFT nftRegistry = VintageNFT(_nftAddress);

        // Check _acceptedCurrency
        require(
            acceptedCurrencies[_currency],
            "Marketplace: Unacceptable marketplace currency"
        );

        // Check if the caller is the owner of the specified token fractions
        for (uint256 i = 0; i < _tokenIds.length; i++) {
            require(
                nftRegistry.balanceOf(msg.sender, _tokenIds[i]) >= _values[i],
                "Marketplace: Insufficient token balance"
            );
        }

        require(
            _expiresAt > block.timestamp.add(1 minutes),
            "Marketplace: Publication should be more than 1 minute in the future"
        );

        // Check if the nft discount is set and active
        if (isNftDiscountActive) {
            for (uint256 i = 0; i < _tokenIds.length; i++) {
                uint256 storedNftDiscount = nftDiscountMapping[_tokenIds[i]];
                if (storedNftDiscount > 0) {
                    _priceInWei = _priceInWei.sub(
                        (_priceInWei.mul(storedNftDiscount)).div(10000)
                    );
                }
            }
        }

        // Transfer the specified fractions from the owner to the marketplace contract
        for (uint256 i = 0; i < _tokenIds.length; i++) {
            nftRegistry.safeTransferFrom(
                msg.sender,
                address(this),
                _tokenIds[i],
                _values[i],
                ""
            );
        }

        // Create the orderId
        bytes32 orderId = keccak256(
            abi.encodePacked(
                block.timestamp,
                msg.sender,
                _nftAddress,
                _tokenIds, // Note: This will not work directly; it needs further serialization
                _priceInWei
            )
        );

        // Save royalty information for the tokenId
        for (uint256 i = 0; i < _tokenIds.length; i++) {
            royaltyBytokenId[_nftAddress][_tokenIds[i]] = _royalty;
        }

        // Save the order
        for (uint256 i = 0; i < _tokenIds.length; i++) {
            orderBytokenId[_nftAddress][_tokenIds[i]] = Order({
                id: orderId,
                seller: msg.sender,
                nftAddress: _nftAddress,
                tokenId: _tokenIds[i],
                value: _values[i],
                price: _priceInWei,
                royalty: _royalty,
                expiresAt: _expiresAt,
                currency: _currency
            });
        }

        emit MultipleOrderCreated(
            orderId,
            msg.sender,
            _nftAddress,
            _tokenIds,
            _values,
            _priceInWei,
            _expiresAt,
            _currency
        );
    }

    function _createResellNft(
        address _nftAddress,
        uint256 _tokenId,
        uint256 _value,
        uint256 _priceInWei,
        uint256 _expiresAt,
        address _currency
    ) internal {
        // Check nft registry
        IERC1155 nftRegistry = IERC1155(_nftAddress);

        // Check order creator is the asset owner
        uint256 tokenvalue = nftRegistry.balanceOf(msg.sender, _tokenId);

        require(
            tokenvalue <= _value,
            "Marketplace: Not holding enough token value!"
        );

        require(_priceInWei > 0, "Marketplace: Price should be bigger than 0");

        require(
            _expiresAt > block.timestamp.add(1 minutes),
            "Marketplace: Publication should be more than 1 minute in the future"
        );

        // get NFT asset from seller to marketplace contract
        nftRegistry.safeTransferFrom(
            msg.sender,
            address(this),
            _tokenId,
            _value,
            ""
        );

        // create the orderId
        bytes32 orderId = keccak256(
            abi.encodePacked(
                block.timestamp,
                msg.sender,
                _nftAddress,
                _tokenId,
                _priceInWei
            )
        );

        // Retrieve royalty from mapping
        uint256 royalty = royaltyBytokenId[_nftAddress][_tokenId];

        // save order
        orderBytokenId[_nftAddress][_tokenId] = Order({
            id: orderId,
            seller: msg.sender,
            nftAddress: _nftAddress,
            tokenId: _tokenId,
            value: _value,
            price: _priceInWei,
            royalty: royalty,
            expiresAt: _expiresAt,
            currency: _currency
        });

        emit OrderCreated(
            orderId,
            msg.sender,
            _nftAddress,
            _tokenId,
            _value,
            _priceInWei,
            _expiresAt,
            _currency
        );
    }

    function _executeOrder(
        bytes32 _orderId,
        address _buyer,
        address _nftAddress,
        uint256 _tokenId,
        uint256 _value,
        uint256 _priceInWei
    ) internal {
        // Update order value
        orderBytokenId[_nftAddress][_tokenId].value -= _value;

        // Check if the remaining value is less than or equal to 0
        if (orderBytokenId[_nftAddress][_tokenId].value <= 0) {
            delete orderBytokenId[_nftAddress][_tokenId];
        }

        // Transfer NFT asset
        IERC1155(_nftAddress).safeTransferFrom(
            address(this),
            _buyer,
            _tokenId,
            _value,
            ""
        );

        // Notify ..
        emit OrderSuccessful(_orderId, _buyer, _tokenId, _value, _priceInWei);
    }

    function _cancelOrder(
        bytes32 _orderId,
        address _nftAddress,
        uint256 _tokenId,
        uint256 _value,
        address _seller
    ) internal {
        require(
            _value <= orderBytokenId[_nftAddress][_tokenId].value,
            "Cancle Bid : Insufficient token value"
        );

        if (_value < orderBytokenId[_nftAddress][_tokenId].value) {
            orderBytokenId[_nftAddress][_tokenId].value -= _value;
        } else {
            delete orderBytokenId[_nftAddress][_tokenId];
        }
        // send asset back to seller
        IERC1155(_nftAddress).safeTransferFrom(
            address(this),
            _seller,
            _tokenId,
            _value,
            ""
        );

        emit OrderCancelled(_orderId);
    }

    function _createBid(
        address _nftAddress,
        uint256 _tokenId,
        uint256 _value,
        uint256 _priceInWei,
        uint256 _expiresAt
    ) internal {
        // Checks order validity
        Order memory order = _getValidOrder(_nftAddress, _tokenId);
        require(_value == 1, "Create Bid: Token value must be 1");

        require(
            orderBytokenId[_nftAddress][_tokenId].value >= _value,
            "Create Bid: Order is not listed"
        );

        // check on expire time
        if (_expiresAt > order.expiresAt) {
            _expiresAt = order.expiresAt;
        }

        // Check price if theres previous a bid
        Bid memory bid = bidByOrderId[_nftAddress][_tokenId];

        // if theres no previous bid, just check price > 0
        if (bid.id != 0) {
            if (bid.expiresAt >= block.timestamp) {
                require(
                    _priceInWei > bid.price,
                    "Marketplace: bid price should be higher than last bid"
                );
            } else {
                require(_priceInWei > 0, "Marketplace: bid should be > 0");
            }

            _cancelBid(
                bid.id,
                _nftAddress,
                _tokenId,
                _value,
                bid.bidder,
                bid.price
            );
        } else {
            require(_priceInWei > 0, "Marketplace: bid should be > 0");
        }

        if (order.currency != 0xEeeeeEeeeEeEeeEeEeEeeEEEeeeeEeeeeeeeEEeE) {
            IERC20(address(order.currency)).transferFrom(
                msg.sender,
                address(this),
                _priceInWei
            );
        }

        // Create bid
        bytes32 bidId = keccak256(
            abi.encodePacked(
                block.timestamp,
                msg.sender,
                order.id,
                _priceInWei,
                _expiresAt
            )
        );

        // Save Bid for this order
        bidByOrderId[_nftAddress][_tokenId] = Bid({
            id: bidId,
            bidder: msg.sender,
            tokenID: _tokenId,
            value: _value,
            price: _priceInWei,
            expiresAt: _expiresAt
        });

        emit BidCreated(
            bidId,
            _nftAddress,
            _tokenId,
            _value,
            msg.sender,
            _priceInWei,
            _expiresAt
        );
    }

    function _cancelBid(
        bytes32 _bidId,
        address _nftAddress,
        uint256 _tokenId,
        uint256 _value,
        address _bidder,
        uint256 _escrowvalue
    ) internal {
        require(_value == 1, "Cancle Bid: Token value must be 1");

        if (_value < bidByOrderId[_nftAddress][_tokenId].value) {
            bidByOrderId[_nftAddress][_tokenId].value -= _value;
        } else {
            delete bidByOrderId[_nftAddress][_tokenId];
        }

        Order memory order = _getValidOrder(_nftAddress, _tokenId);

        // return escrow to canceled bidder
        order.currency == MARKETPLACE_ETHER
            ? payable(_bidder).transfer(_escrowvalue)
            : IERC20(order.currency).safeTransfer(_bidder, _escrowvalue);

        emit BidCancelled(_bidId);
    }

    function _getValidOrder(address _nftAddress, uint256 _tokenId)
        internal
        view
        returns (Order memory order)
    {
        order = orderBytokenId[_nftAddress][_tokenId];

        require(order.id != 0, "Marketplace: asset not published");
        require(
            order.expiresAt >= block.timestamp,
            "Marketplace: order expired"
        );

        return order;
    }

    function setCommission(uint256 _newCommissionPrice) public onlyOwner {
        commission = _newCommissionPrice;
    }

    function withdraw(uint256 _amount) external onlyOwner nonReentrant {
        uint256 contractBalance = address(this).balance;
        require(
            contractBalance >= _amount,
            "Marketplace: amount is greater than contract balance"
        );
        payable(msg.sender).transfer(_amount);
    }
}