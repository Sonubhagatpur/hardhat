pragma solidity ^0.8.0;

contract Discount {
    struct NFTDiscount {
        uint256 assetId;
        uint256 discountPercentage;
    }

    mapping(address => mapping(uint256 => uint256)) public nftDiscounts;

    event NFTDiscountSet(
        address indexed nftAddress,
        uint256 assetId,
        uint256 discountPercentage
    );

    function getERC20Discount(address _currency) public view returns (uint256) {
        return 0;
    }

    /**
     * @dev Set NFT discount for a specific asset.
     * @param _nftAddress Address of the NFT contract.
     * @param _assetId ID of the NFT asset.
     * @param _discountPercentage Discount percentage for the asset.
     */
    function setNFTDiscount(
        address _nftAddress,
        uint256 _assetId,
        uint256 _discountPercentage
    ) external {
        require(_nftAddress != address(0), "Invalid NFT address");
        require(_discountPercentage <= 10000, "Invalid discount percentage");

        nftDiscounts[_nftAddress][_assetId] = _discountPercentage;
        emit NFTDiscountSet(_nftAddress, _assetId, _discountPercentage);
    }
}
