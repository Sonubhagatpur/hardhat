interface IMarketplace {
    struct Order {
        bytes32 id;
        address seller;
        address nftAddress;
        uint256 tokenId;
        uint256 value;
        uint256 price;
        uint256 royalty;
        uint256 expiresAt;
        address currency;
    }

    struct Bid {
        bytes32 id;
        address bidder;
        uint256 tokenID;
        uint256 value;
        uint256 price;
        uint256 expiresAt;
    }

    event OrderCreated(
        bytes32 id,
        address indexed seller,
        address indexed nftAddress,
        uint256 tokenId,
        uint256 value,
        uint256 price,
        uint256 expiresAt,
        address currency
    );

    event MultipleOrderCreated(
        bytes32 indexed orderId,
        address indexed seller,
        address indexed nftAddress,
        uint256[] tokenIds,
        uint256[] values,
        uint256 priceInWei,
        uint256 expiresAt,
        address currency
    );

    event OrderSuccessful(
        bytes32 id,
        address indexed buyer,
        uint256 _tokenId,
        uint256 _value,
        uint256 _priceInWei
    );
    event OrderUpdated(
        bytes32 id,
        uint256 tokenId,
        uint256 value,
        uint256 priceInWei,
        uint256 expiresAt
    );
    event OrderCancelled(bytes32 id);

    // BID EVENTS
    event BidCreated(
        bytes32 id,
        address indexed nftAddress,
        uint256 indexed tokenId,
        uint256 value,
        address indexed bidder,
        uint256 priceInWei,
        uint256 expiresAt
    );
    event BidAccepted(bytes32 id);
    event BidCancelled(bytes32 id);
}

contract Marketplace is
