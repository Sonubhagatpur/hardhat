            "Only the minter can increase fractions"
        );
        require(_value > 0, "value must be greater than 0");

        _fractions[_tokenId][msg.sender] += _value;

        _mint(msg.sender, _tokenId, _value, _data);

        emit FractionMinted(msg.sender, _tokenId, _value);
    }

    function withdraw(uint256 _amount) external onlyOwner {
        uint256 contractBalance = address(this).balance;
        require(
            contractBalance >= _amount,
            "VintageNFT: amount is greater than contract balance"
        );
        payable(msg.sender).transfer(_amount);
    }
}

